<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="#14262B"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.cardview.widget.CardView
        android:id="@+id/reminderCV"
        android:layout_width="113dp"
        android:layout_height="114dp"
        android:layout_marginStart="68dp"
        app:cardBackgroundColor="#26444E"
        app:cardCornerRadius="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imsakiyeCV"
        app:layout_constraintEnd_toEndOf="@+id/religiousDayNightsCV"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imsakiyeCV"
        app:layout_constraintTop_toTopOf="@+id/imsakiyeCV"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView17"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/hatirlaticimm" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:gravity="center_horizontal"
                android:text="Ezan Vakti Hatırlatıcı"
                android:textColor="@color/white"
                android:textSize="11sp"
                android:textStyle="bold"
                app:autoSizeTextType="uniform" />
        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/imsakiyeCV"
        android:layout_width="113dp"
        android:layout_height="114dp"
        android:layout_marginTop="16dp"
        app:cardBackgroundColor="#26444E"
        app:cardCornerRadius="8dp"
        app:layout_constraintEnd_toEndOf="@+id/salaryPrayTimeCV"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/salaryPrayTimeCV"
        app:layout_constraintTop_toTopOf="@+id/guideline18">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/imsakiyem" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:gravity="center_horizontal"
                android:text="İmsakiye"
                android:textColor="@color/white"
                android:textSize="11sp"
                android:textStyle="bold"
                app:autoSizeTextType="uniform" />

        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/religiousDayNightsCV"
        android:layout_width="113dp"
        android:layout_height="114dp"
        android:layout_marginStart="84dp"
        app:cardBackgroundColor="#26444E"
        app:cardCornerRadius="8dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline18"
        app:layout_constraintEnd_toStartOf="@+id/guideline121"
        app:layout_constraintStart_toStartOf="@+id/guideline271"
        app:layout_constraintTop_toTopOf="@+id/salaryPrayTimeCV"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                app:srcCompat="@drawable/adsiz" />

            <TextView
                android:id="@+id/textView44"
                android:layout_width="103dp"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:gravity="center_horizontal"
                android:text="Dini Günler ve Geceler"
                android:textColor="@color/white"
                android:textSize="11sp"
                android:textStyle="bold"
                app:autoSizeTextType="uniform" />
        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline17"
        android:layout_width="113dp"
        android:layout_height="114dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="26dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="144dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline269"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.43" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline42"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.27" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline270"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_end="114dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline43"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.39" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline75"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.16" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline271"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.34" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline272"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.66" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline121"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.82" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.49" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.57" />


    <androidx.cardview.widget.CardView
        android:id="@+id/basicReligiousKnowledgeCv"
        android:layout_width="113dp"
        android:layout_height="114dp"
        android:layout_marginTop="16dp"
        app:cardBackgroundColor="#26444E"
        app:cardCornerRadius="8dp"
        app:layout_constraintEnd_toEndOf="@+id/imsakiyeCV"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/imsakiyeCV"
        app:layout_constraintTop_toBottomOf="@+id/imsakiyeCV">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="match_parent"
                android:layout_height="101dp"
                android:layout_weight="1"
                android:background="@drawable/temeldinibilgim" />

            <TextView
                android:id="@+id/textView35"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:gravity="center_horizontal"
                android:text="Temel Dini Bilgiler"
                android:textColor="@color/white"
                android:textSize="11sp"
                android:textStyle="bold"
                app:autoSizeTextType="uniform" />

        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/salaryPrayTimeCV"
        android:layout_width="113dp"
        android:layout_height="114dp"
        app:cardBackgroundColor="#26444E"
        app:cardCornerRadius="8dp"
        app:layout_constraintStart_toStartOf="@+id/guideline75"
        app:layout_constraintTop_toTopOf="@+id/guideline17">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:orientation="vertical">


            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="match_parent"
                android:layout_height="28dp"
                android:layout_weight="1"
                app:srcCompat="@drawable/namazvaktimm" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:gravity="center_horizontal"
                android:text="Aylık Namaz Vakitleri"
                android:textColor="@color/white"
                android:textSize="11sp"
                android:textStyle="bold"
                app:autoSizeTextType="uniform" />
        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/kiblepusula"
        android:layout_width="113dp"
        android:layout_height="114dp"
        android:layout_marginStart="68dp"
        app:cardBackgroundColor="#26444E"
        app:cardCornerRadius="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/basicReligiousKnowledgeCv"
        app:layout_constraintEnd_toEndOf="@+id/reminderCV"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/basicReligiousKnowledgeCv"
        app:layout_constraintTop_toTopOf="@+id/basicReligiousKnowledgeCv"
        app:layout_constraintVertical_bias="0.0">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/image"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@drawable/kiblebulucum" />

            <TextView
                android:id="@+id/text"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:gravity="center_horizontal"
                android:text="Kıble Bulucu"
                android:textColor="@color/white"
                android:textSize="11sp"
                android:textStyle="bold"
                app:autoSizeTextType="uniform" />

        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/zikirmatik"
        android:layout_width="113dp"
        android:layout_height="114dp"
        app:cardBackgroundColor="#26444E"
        android:layout_marginTop="16dp"
        app:cardCornerRadius="8dp"
        app:layout_constraintBottom_toTopOf="@+id/guideline270"
        app:layout_constraintEnd_toStartOf="@+id/guideline40"
        app:layout_constraintHorizontal_bias="0.526"
        app:layout_constraintStart_toEndOf="@+id/basicReligiousKnowledgeCv"
        app:layout_constraintTop_toBottomOf="@+id/basicReligiousKnowledgeCv"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_margin="5dp"
            android:orientation="vertical">


            <ImageView
                android:id="@+id/imageView15"
                android:layout_width="match_parent"
                android:layout_height="82dp"
                android:src="@drawable/zikirmatik_png" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="2dp"
                android:gravity="center_horizontal"
                android:text="Zikirmatik"
                android:textColor="@color/white"
                android:textSize="11sp"
                android:textStyle="bold"
                app:autoSizeTextType="uniform" />
        </LinearLayout>


    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>